pipeline {
    //agent any
    triggers {
        pollSCM '* * * * *'
      }
    agent {
        docker { image 'ubuntu' }
    }
    stages {
        stage('Deploy ubuntu') {
            steps {
                 
        sh """  
                ls -al
                sudo apt-get install python
                
           
                ls -al
                
          """    
            }
        }
    }
}
